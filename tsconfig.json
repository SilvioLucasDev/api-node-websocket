{
  "compilerOptions": {
    "outDir": "dist",
    "rootDirs": ["src"],
    "target": "es2021",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
  },
  "include": ["src"]
}
